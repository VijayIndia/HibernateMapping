<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.database.common.model.date.EmployeeWithDateTime" table="employeeWithDateTime">
        <id name="employeeId" column="employee_id">
            <generator class="increment"/>
        </id>
        <property name="employeeName" column="employee_name" type="java.lang.String"/>
        <property name="isoDateTime" column="iso_date_time" type="java.lang.String" />
        <property name="startDate" column="start_date" type="java.util.Date"/>
    </class>
</hibernate-mapping>